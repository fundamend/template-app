{
	"name": "@template-app/page-www",
	"version": "0.0.0",
	"dependencies": {
		"@fundamend/components-layout": "^0.0.2",
		"@fundamend/css": "^0.0.1",
		"@fundamend/css-theme-playful": "^0.0.1",
		"@fundamend/service-crash-sentry-browser": "^0.0.0",
		"@fundamend/service-dependency-container-awilix": "^0.0.0"
	},
	"devDependencies": {
		"@cucumber/cucumber": "^8.8.0",
		"@fundamend/config-eslint": "^0.1.1",
		"@fundamend/config-lint-staged": "^0.1.1",
		"@fundamend/config-prettier": "^0.1.1",
		"@fundamend/config-remark": "^0.1.1",
		"@fundamend/config-standard-version": "^0.1.1",
		"@fundamend/config-stylelint": "^0.1.1",
		"@fundamend/config-vitest": "0.0.0",
		"@playwright/test": "^1.30.0",
		"@size-limit/preset-app": "^8.1.2",
		"astro": "^1.6.14",
		"concurrently": "^7.2.2",
		"conventional-github-releaser": "3.1.3",
		"cross-env": "^7.0.3",
		"eslint": "^8.2.0",
		"lerna": "^6.0.3",
		"lint-staged": "^12.1.2",
		"madge": "^5.0.1",
		"playwright": "^1.30.0",
		"prettier": "^2.7.1",
		"remark-cli": "^10.0.0",
		"rimraf": "^3.0.2",
		"size-limit": "^8.1.2",
		"stylelint": "^13.13.1",
		"vitest": "^0.25.3",
		"wait-on": "^6.0.1",
		"wrangler": "^2.9.1"
	},
	"license": "MIT",
	"main": "src/pages.index.astro",
	"scripts": {
		"clean": "rimraf build *.log",
		"clean:all": "yarn clean && rimraf node_modules",
		"dev": "astro dev",
		"graph": "madge --image graph.png src",
		"lint": "yarn lint:js && yarn lint:css && yarn lint:md && yarn lint:formatting",
		"lint:css": "stylelint **/*.css --allow-empty-input --ignore-path .gitignore",
		"lint:css:fix": "stylelint **/*.css --allow-empty-input --ignore-path .gitignore --fix",
		"lint:fix": "yarn lint:js:fix && yarn lint:css:fix && yarn lint:md:fix && yarn lint:formatting:fix",
		"lint:formatting": "prettier . --check",
		"lint:formatting:fix": "prettier . --write",
		"lint:js": "eslint .",
		"lint:js:fix": "eslint . --fix",
		"lint:md": "remark . --ignore-path .gitignore",
		"lint:md:fix": "remark . --ignore-path .gitignore --output",
		"precommit": "lint-staged",
		"prod": "astro build",
		"prod:watch": "wrangler pages dev build --live-reload",
		"release:prepare": "cross-env-shell \"standard-version -t $npm_package_name@\"",
		"size": "yarn prod && size-limit",
		"size:why": "yarn prod && size-limit --why",
		"test": "yarn test:unit && yarn test:behavior",
		"test:behavior": "yarn prod && concurrently --names wrangler,cucumberjs --prefix name --prefix-colors yellow,green --success first --kill-others \"wrangler pages dev build --local --port 1351\" \"wait-on http://localhost:1351 && cucumber-js\"",
		"test:unit": "vitest --run --config vitestrc.js"
	},
	"type": "module"
}
